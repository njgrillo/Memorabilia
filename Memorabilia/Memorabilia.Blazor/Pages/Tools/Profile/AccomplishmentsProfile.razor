@inherits PersonProfile 

<MudTable Bordered="false"
          Dense="false"
          Filter="new Func<AccomplishmentProfileModel,bool>(FilterFunc1)"
          Hover="true"
          Items="@Accomplishments"
          Striped="true">
    <ToolBarContent>
        <ToolbarTitle Text="Accomplishments" />
        <MudSpacer />
        <ToolbarSearchField SearchText="@_search"
                            SearchTextChanged="@((p) => _search = p)" />
    </ToolBarContent>
    <HeaderContent>
        <MudTh>Name</MudTh>
        <MudTh>Date/Year</MudTh>
    </HeaderContent>
    <RowTemplate>
        <MudTd DataLabel="Name">@context.AccomplishmentTypeName</MudTd>
        <MudTd DataLabel="Date/Year">@context.TimeFrame</MudTd>
    </RowTemplate>
    <PagerContent>
        <MudTablePager />
    </PagerContent>
</MudTable>