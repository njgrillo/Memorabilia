<Div Class="row">
    <Div Class="col-3">
        <Text Typography="Typo.h3">
            Team
        </Text>
    </Div>
</Div>
<Div Class="row">
    <Div Class="col-3">
        <TeamAutoComplete Disabled="Disabled" 
                          SelectedValue="SelectedTeam"
                          SelectionChanged="(p) => TeamChanged(p)" />
    </Div>
    <Div Class="col-3">
        <NullableIntField Disabled="Disabled" 
                          Label="Year"
                          Value="Year"
                          ValueChanged="@((p) => YearChanged(p))" />
    </Div>
</Div>