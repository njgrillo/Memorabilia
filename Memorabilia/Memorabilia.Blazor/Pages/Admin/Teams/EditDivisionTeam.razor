@inherits EditTeamItem<TeamDivisionsEditModel, TeamDivisionModel>

<PageEditor EditModel="EditModel"
            OnSave="Save"
            PageImageFileName="@EditModel.ImageFileName"
            TItem="TeamDivisionsEditModel">
    <TimelineContent>
        <TeamTimeline DisplayConference="EditModel.CanHaveConference" 
                      EditMode="EditModel.EditModeType"
                      SportLeagueLevel="EditModel.SportLeagueLevel"
                      TeamId="TeamId"
                      TeamStep="EditModel.TeamStep" />
    </TimelineContent>
    <ChildContent>
        <DivRowCol12>
            <TeamDivisionEditor Divisions="EditModel.Divisions"
                                SportLeagueLevel="EditModel.SportLeagueLevel" />
        </DivRowCol12>
        <br />
    </ChildContent>
</PageEditor>