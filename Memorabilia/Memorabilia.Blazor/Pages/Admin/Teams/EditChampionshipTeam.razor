@inherits EditTeamItem<TeamChampionshipsEditModel, TeamChampionshipModel>

<PageEditor ContinueNavigationPath="@EditModel.ContinueNavigationPath" 
            DisplaySaveContinueButton="false"          
            EditModel="EditModel"
            ExitNavigationPath="@EditModel.ExitNavigationPath"
            OnSave="Save"
            PageImageFileName="@EditModel.ImageFileName"
            TItem="TeamChampionshipsEditModel">
    <TimelineContent>
        <TeamTimeline DisplayConference="EditModel.CanHaveConference" 
                      EditMode="EditModel.EditModeType"
                      SportLeagueLevel="EditModel.SportLeagueLevel"
                      TeamId="TeamId"
                      TeamStep="EditModel.TeamStep" />
    </TimelineContent>
    <ChildContent>
        <DivRowCol12>
            <TeamChampionshipEditor Championships="EditModel.Championships"
                                    SportLeagueLevel="EditModel.SportLeagueLevel" />
        </DivRowCol12>
        <br />
    </ChildContent>
</PageEditor>
