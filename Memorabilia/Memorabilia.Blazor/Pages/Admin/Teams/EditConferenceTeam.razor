@inherits EditTeamItem<TeamConferencesEditModel, TeamConferenceModel>

<PageEditor EditModel="EditModel"
            OnSave="Save"
            PageImageFileName="@EditModel.ImageFileName"
            TItem="TeamConferencesEditModel">
    <TimelineContent>
        <TeamTimeline DisplayConference="EditModel.CanHaveConference" 
                      EditMode="EditModel.EditModeType"
                      SportLeagueLevel="EditModel.SportLeagueLevel"
                      TeamId="TeamId"
                      TeamStep="EditModel.TeamStep" />
    </TimelineContent>
    <ChildContent>
        <DivRowCol12>
            <TeamConferenceEditor Conferences="EditModel.Conferences"
                                  SportLeagueLevel="EditModel.SportLeagueLevel" />
        </DivRowCol12>
        <br />
    </ChildContent>
</PageEditor>