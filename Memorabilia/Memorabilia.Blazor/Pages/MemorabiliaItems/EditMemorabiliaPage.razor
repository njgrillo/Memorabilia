@typeparam TItem

@inherits MemorabiliaItem<TItem>

<PageEditor ContinueNavigationPath="@ContinueNavigationPath" 
            EditModel="EditModel"
            ExitNavigationPath="@ExitNavigationPath"
            OnSave="Save"
            PageImageFileName="@EditModel.ImageFileName"
            TItem="TItem">
    <TimelineContent>
        <MemorabiliaItemTimeline EditMode="EditModel.EditModeType"
                                 ItemTypeName="@EditModel.ItemType.Name"
                                 MemorabiliaId="EditModel.MemorabiliaId"
                                 MemorabiliaItemStep="EditModel.MemorabiliaItemStep" />
    </TimelineContent>
    <ChildContent>
        @Content
    </ChildContent>
</PageEditor>
