<DivPad5>
    <MudCard>
        <MudCardMedia Height="300"
                      Image="@(ImageService.GetDomainImageData(ImageFileName))" />
        <MudCardContent>
            <Text Typography="Typo.h5">@Title</Text>
            <Text Typography="Typo.body2">@Description</Text>
        </MudCardContent>
        <MudCardActions>
            <CustomButton Color="MudBlazor.Color.Primary"
                          OnClick="OnClick"
                          Text="View"
                          Variant="Variant.Text" />
        </MudCardActions>
    </MudCard>
</DivPad5>
